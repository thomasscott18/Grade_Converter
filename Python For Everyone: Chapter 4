# This code takes a numerical score and turns it into a letter grade

grade = input("Enter your score out of 100: ")

try:
  x = float(grade)
  if x < 0:
    print("Please enter a number between 0 and 100")
  elif x < 60:
    print("F")
  elif x < 70:
    print("D")
  elif x < 80:
    print("C")
  elif x < 90:
    print("B")
  elif x < 100:
    print("A")
  elif x > 100:
    print("Please enter a number between 0 and 100")
except:
  print("Please enter a number between 0 and 100")
